						"exec": [
							"var jsnData = pm.response.json();\r",
							"pm.environment.set(\"ID\", jsnData.bookingid)\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"First name check\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.booking.firstname).to.eql(\"Rafat\");\r",
							"});\r",
							"pm.test(\"Last name check\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.booking.lastname).to.eql(\"Islam\");\r",
							"});\r",
							"pm.test(\"Last name check\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.booking.totalprice).to.eql(1000);\r",
							"});\r",
							"pm.test(\"Last name check\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.booking.additionalneeds).to.eql(\"Breakfast\");\r",
							"});\r",
							"\r",
							"pm.test(\"Chek In check\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.booking.bookingdates.checkin).to.eql(\"2018-01-01\");\r",
							"});\r",
							"pm.test(\"Chek In check\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.booking.bookingdates.checkout).to.eql(\"2019-01-01\");\r",
							"});\r",
							"\r",
							"\r",
							""
